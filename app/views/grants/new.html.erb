<h1>New grant</h1>

<% form_for(@grant) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :grant_name %><br />
    <%= f.text_field :grant_name %>
  </p>
  <p>
    <%= f.label :grant_contrl %><br />
    <%= f.text_field :grant_contrl %>
  </p>
  <p>
    <%= f.label :grant_action %><br />
    <%= f.text_field :grant_action %>
  </p>
  <p>
    <%= f.label :grant_target %><br />
    <%= f.text_field :grant_target %>
  </p>
  <p>
    <%= f.label :grant_desc %><br />
    <%= f.text_area :grant_desc %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', grants_path %>
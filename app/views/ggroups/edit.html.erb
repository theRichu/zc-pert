<h1>Editing ggroup</h1>

<% form_for(@ggroup) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :gg_name %><br />
    <%= f.text_field :gg_name %>
  </p>
  <p>
    <%= f.label :gg_desc %><br />
    <%= f.text_field :gg_desc %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @ggroup %> |
<%= link_to 'Back', ggroups_path %>
<h1>Listing mappings</h1>

<table>
  <tr>
    <th>Xls column</th>
    <th class="th_last">Staff attr</th>
  </tr>

<% @mappings.each do |mapping| %>
  <tr>
    <td><%=h mapping.xls_column %></td>
    <td><%=h mapping.staff_attr %></td>
    <td><%= link_to 'Show', mapping %></td>
    <td><%= link_to 'Edit', edit_mapping_path(mapping) %></td>
    <td><%= link_to 'Destroy', mapping, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New mapping', new_mapping_path %>